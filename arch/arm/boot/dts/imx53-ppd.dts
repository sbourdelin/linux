	spidev0: spi@0 {
		/* programming interface, requires reset */
		compatible = "ge,achc";
		reg = <0>;
		spi-max-frequency = <1000000>;
		status = "okay";
	};

	spidev1: spi@1 {
		compatible = "ge,achc";
		reg = <1>;
		spi-max-frequency = <1000000>;
		status = "okay";
	};

	gpioxra0: gpio@2 {
		compatible = "exar,xra1403";
		reg = <2>;
		gpio-controller;
		#gpio-cells = <2>;
		spi-max-frequency = <1000000>;
		status = "okay";
	};

	gpioxra1: gpio@3 {
		compatible = "exar,xra1403";
		reg = <3>;
		gpio-controller;
		#gpio-cells = <2>;
		spi-max-frequency = <1000000>;
		status = "okay";
	};
