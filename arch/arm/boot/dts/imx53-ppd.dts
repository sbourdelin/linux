				MX53_PAD_EIM_DA7__GPIO3_7		NO_PAD_CTL
				/* SOFTWARE_CONTROLLED_PWR_CYCLE */
				MX53_PAD_EIM_DA8__GPIO3_8		NO_PAD_CTL
				/* SOFTWARE_CONTROLLED_POWERDOWN */
				MX53_PAD_EIM_DA9__GPIO3_9		NO_PAD_CTL
