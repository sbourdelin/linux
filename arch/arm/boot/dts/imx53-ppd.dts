				/* RESET_IMX535_USB_HOST2_PHY_N */
				MX53_PAD_GPIO_14__GPIO4_4		NO_PAD_CTL
				/* ONKEY_IS_DEPRESSED */
				MX53_PAD_KEY_ROW3__GPIO4_13		NO_PAD_CTL
				/* UNUSED_GPIO_TO_ALARM_LIGHT_BOARD */
				MX53_PAD_EIM_WAIT__GPIO5_0		NO_PAD_CTL
