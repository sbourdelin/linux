#include "mt7623-mt6323.dtsi"

/ {
	chosen {
		stdout-path = "serial2:115200n8";
	};

	memory@80000000 {
		reg = <0 0x80000000 0 0x40000000>;
	};

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
	};
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};
