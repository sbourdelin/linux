#include "s500.dtsi"

/ {
	compatible = "lemaker,guitar", "actions,s500";

	/delete-node/ memory;

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x40000000>;
	};
};

&timer {
	clocks = <&hosc>;
};
